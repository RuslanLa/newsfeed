bs_package_flags = -bs-package-name bs-jest
src_root_dir = /home/ruslan/newsfeed/node_modules/bs-jest
bsc = /home/ruslan/.npm-global/lib/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/ruslan/.npm-global/lib/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -nostdlib -I '/home/ruslan/newsfeed/node_modules/bs-platform/lib/ocaml' -bs-no-version-header -bs-super-errors -no-alias-deps -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
bsc_group_1_includes = 
bsc_lib_includes = -I src
rule build_ast_and_module_sets
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.mliast : build_ast_and_module_sets $src_root_dir/src/jest.mli
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.mliast.d : build_deps src/jest.mliast
rule build_cmi
  command = ${bsc} ${bs_package_flags} -bs-no-builtin-ppx-mli -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.cmi : build_cmi src/jest.mliast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build src/jest.mlast : build_ast_and_module_sets $src_root_dir/src/jest.ml
build src/jest.mlast.d : build_deps src/jest.mlast
rule build_cmj_only
  command = ${bsc} ${bs_package_flags} -bs-assume-has-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include  ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/jest.cmj |  $src_root_dir/lib/js/src/jest.js : build_cmj_only src/jest.mlast |  src/jest.cmi
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/src
build build.ninja : phony  ||  src/jest.mlast.d src/jest.mliast.d
